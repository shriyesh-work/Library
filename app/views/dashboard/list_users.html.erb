<h1>List of users</h1>
<% if @users %>
<p>
<%= link_to "prev", dashboard_list_users_path(count: @prev) %> |
<%= link_to "next", dashboard_list_users_path(count: @count) %></p>
<table border="2">
  <tr><th>User id</th><th>User Name</th><th>User Email</th><th> Settings </th></tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.firstname %> <%= user.lastname %></td>
      <td><%= user.email %></td>
      <td>
        <%= link_to "Edit", edit_user_path(user) %> | 
        <% unless user.is_admin %>
           Delete
        <% end %>
        </td>
     </tr>
  <% end %>
</table>
<% end %>

